{
  "name": "display-my-framework",
  "version": "0.1.3",
  "description": "A minimal React JS framework to get started.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/lib/index.js",
  "keywords": [
    "react",
    "framework",
    "web"
  ],
  "dependencies": {
    "deepmerge": "^4.2.2"    
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "author": "Display Table",
  "license": "MIT",
  "repository": "https://github.com/DisplayTable/display-my-framework.git",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "npm-run-all --parallel build-watch react-scripts",
    "react-scripts": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "i-all": "npm i && npm-install-peers"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.70.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  }
}
